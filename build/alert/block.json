{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/alert",
  "version": "0.1.0",
  "title": "Alert",
  "category": "theme",
  "icon": "index-card",
  "description": "Single Page Alert Box",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "selector": ".alert-title",
      "default": ""
    },
    "content": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "alert",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/content-carousel-inner-block",
  "parent": [
    "create-block/content-carousel"
  ],
  "version": "0.1.0",
  "title": "Content Carousel Slide",
  "category": "theme",
  "icon": "index-card",
  "description": "",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "selector": "h2",
      "default": ""
    },
    "subTitle": {
      "type": "string",
      "selector": "p",
      "default": ""
    },
    "link": {
      "type": "string",
      "default": ""
    },
    "linkLabel": {
      "type": "string",
      "default": "Check it out!"
    },
    "hasLinkNofollow": {
      "type": "boolean",
      "default": false
    },
    "openNewTab": {
      "type": "boolean",
      "default": false
    },
    "mediaID": {
      "type": "number",
      "default": ""
    },
    "mediaURL": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "content-carousel-inner-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
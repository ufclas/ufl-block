{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/showcase-text-only",
	"version": "0.1.0",
	"title": "Showcase Text Only",
	"category": "theme",
	"icon": "index-card",
	"description": "",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": "h2",
			"default": ""
		},
		"content": {
			"type": "string",
			"selector": "p",
			"default": ""
		},		
		"linkLabel": {
			"type": "string",
			"default": ""
		},		
		"link": {
			"type": "string",
			"selector": ".animated-border-button",
			"default": ""
		},
		"hasLinkNofollow": {
			"type": "boolean",
			"default": false
		},
		"openNewTab": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "showcase-text-only",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
